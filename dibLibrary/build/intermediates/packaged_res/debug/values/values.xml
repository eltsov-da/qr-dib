    <color name="accentDark">#d01716</color>
    <color name="accentLight">#f9bdbb</color>
    <color name="actionBarText">@android:color/white</color>
    <color name="hintText">#42000000</color>
    <color name="primary">#5677fc</color>
    <color name="primaryDark">#455ede</color>
    <color name="primaryLight">#d0d9ff</color>
    <color name="secondaryText">#89000000</color>
    <color name="text">#cc000000</color>
    <dimen name="body1">14sp</dimen>
    <dimen name="dpMinimum">1dp</dimen>
    <dimen name="grid">8dp</dimen>
    <dimen name="halfGrid">4dp</dimen>
    <dimen name="margin">16dp</dimen>
    <dimen name="subhead">16sp</dimen>
    <string name="app_name">dib Library</string>
    <string name="dnf">dnf</string>
    <string name="dns">dns</string>
    <string name="dwn">DWN</string>
    <string name="fin">FIN</string>
    <string name="finish">Финиш</string>
    <string name="library_name">dib Library</string>
    <string name="mp">mp</string>
    <string name="nullTime">--:--:--</string>
    <string name="ok">OK</string>
    <string name="prefdef_class">Class</string>
    <string name="prefdef_club">Club</string>
    <string name="prefdef_course_length">0</string>
    <string name="prefdef_course_name">Course</string>
    <string name="prefdef_forename">Forename</string>
    <string name="prefdef_id">ID</string>
    <string name="prefdef_surname">Surname</string>
    <string name="rad">RAD</string>
